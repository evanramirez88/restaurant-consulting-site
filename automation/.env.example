# Toast ABO Worker Configuration
# Copy this file to .env and fill in your values

# API Configuration
API_BASE_URL=https://ccrestaurantconsulting.com
WORKER_API_KEY=your-worker-api-key-here

# Encryption key for decrypting Toast credentials
# Must match the key used in Cloudflare backend
ENCRYPTION_KEY=your-encryption-key-here

# Browser Settings
HEADLESS=true
SLOW_MO=50

# Job Processing
MAX_SESSIONS=2

# Logging
LOG_LEVEL=info
